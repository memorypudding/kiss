# XSINT Configuration
# Copy this file to .env and fill in your API keys
# Alternatively, create ~/.xsint/config.json

# Have I Been Pwned API Key
# Get one at: https://haveibeenpwned.com/API/Key
XSINT_HIBP_API_KEY=

# IPStack API Key (for IP geolocation)
# Get one at: https://ipstack.com
XSINT_IPSTACK_KEY=

# Predicta Search API Key
XSINT_PREDICTA_KEY=

# Veriphone API Key (for phone validation)
XSINT_VERIPHONE_KEY=

# SherlockEye API Key
XSINT_SHERLOCKEYE_KEY=

# Email Validator API Key
XSINT_EMAIL_VALIDATOR_KEY=
